# Flask AI Inference Server Configuration

# 모델 디렉토리 경로
MODEL_DIR=./final_model

# API 키 (Django의 FLASK_API_KEY와 동일해야 함)
# 프로덕션에서는 반드시 변경하세요
API_KEY=development-api-key-change-in-production

# Flask 서버 포트
# Django ml_proxy 설정과 일치해야 함
PORT=9000

# 로그 레벨 (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
